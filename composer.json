{
    "name": "laravel-liberu/tutorials",
    "description": "Tutorial management dependency for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "tutorials",
        "tutorial-manager",
        "bootstrap-tour"
    ],
    "homepage": "https://github.com/laravel-liberu/Tutorials",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/dynamic-methods": "^2.0",
        "laravel-liberu/enums": "^2.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/migrator": "^2.0",
        "laravel-liberu/permissions": "^5.0",
        "laravel-liberu/tables": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Tutorials\\": "src/",
            "LaravelEnso\\Tutorials\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Tutorials\\AppServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
